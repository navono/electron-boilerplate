{
  "name": "config-platform",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env PORT=3080 NODE_ENV=development node server.js",
    "vs-debug": "yarn dev --debug",
    "electron": "cross-env NODE_ENV=development electron ./app/main/index.js",
    "build": "yarn clean && yarn build:main && yarn build:renderer",
    "build:main": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.main.prod.js --progress --profile --colors",
    "build:renderer": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.renderer.prod.js --progress --profile --colors",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "antd": "^3.4.1",
    "electron": "^1.8.4",
    "konva": "^2.0.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-konva": "^1.7.2"
  },
  "devDependencies": {
    "@types/react": "^16.3.11",
    "@types/react-beautiful-dnd": "^6.0.1",
    "@types/react-dom": "^16.0.5",
    "@types/react-hot-loader": "^3.0.6",
    "autoprefixer": "^8.3.0",
    "babel-loader": "^7.1.4",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "electron-devtools-installer": "^2.2.3",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "2.7.2",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.2.1",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-loader": "^2.1.4",
    "prettier": "^1.12.1",
    "react-beautiful-dnd": "^7.0.1",
    "react-hot-loader": "^4.0.1",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "ts-import-plugin": "^1.4.4",
    "ts-loader": "^4.2.0",
    "tslint": "^5.9.1",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
