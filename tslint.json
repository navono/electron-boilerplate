{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "prettier": true,
    "semicolon": false,
    "quotemark": [true, "single", "jsx-double"],
    "object-literal-sort-keys": false,
    "no-implicit-dependencies": [true, "dev"]
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}